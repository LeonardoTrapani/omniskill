{
  "name": "@omniscient/scraping",
  "private": true,
  "type": "module",
  "scripts": {
    "scrape:ingest": "bun run src/ingest-skills.ts --dir ./parsed-skills -v",
    "scrape:import-missing": "bun run src/import-missing-resources.ts --dir ./parsed-skills -v",
    "scrape:resolve-refs": "bun run src/resolve-resource-refs.ts -v",
    "scrape:sync-links": "bun run src/sync-all-links.ts -v",
    "scrape:pipeline": "bun run src/ingest-skills.ts --dir ./parsed-skills -v && bun run src/import-missing-resources.ts --dir ./parsed-skills -v && bun run src/resolve-resource-refs.ts -v && bun run src/sync-all-links.ts -v"
  },
  "dependencies": {
    "@omniscient/db": "workspace:*",
    "drizzle-orm": "^0.45.1",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@omniscient/config": "workspace:*",
    "@types/bun": "catalog:",
    "typescript": "catalog:"
  }
}
